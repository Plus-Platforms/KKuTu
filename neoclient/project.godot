config_version=5

[application]

config/name="Plus KKuTu"
config/name_localized={
"en": "Plus KKuTu",
"ko": "플러스끄투"
}
config/version="0.01"
run/main_scene="res://intro.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
